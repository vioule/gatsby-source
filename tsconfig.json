{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": ["es2015", "es2019", "esnext"],
    "outDir": "",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "experimentalDecorators": true
  },
  "exclude": ["src/**/**.test.ts"],
  "include": ["src/**/*"]
}
